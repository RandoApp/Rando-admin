{
  "name": "rando-admin",
  "version": "0.0.1",
  "description": "Admin console for rando server",
  "engine": {
    "node": ">=0.8.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RandoApp/Rando-admin.git"
  },
  "dependencies": {
    "command-line-args": "3.0.0",
    "mongodb": "2.1.18",
    "mongoose": "4.5.2",
    "express": "4.14.0",
    "serve-static": "1.11.1",
    "body-parser": "1.15.2",
    "js-yaml": "3.6.1",
    "config": "1.21.0",
    "winston": "2.2.0",
    "async": "2.0.0-rc.6",
    "mv": "2.1.1",
    "gm": "1.22.0",
    "s3": "4.4.0",
    "diskspace": "1.0.3",
    "basic-auth": "1.0.4",
    "randoDB": "git+https://github.com/RandoApp/Rando-db.git"
  },
  "devDependencies": {
    "mocha": "2.5.3",
    "should": "9.0.2",
    "sinon": "1.17.4",
    "istanbul": "0.4.4",
    "coveralls": "2.11.9"
  },
  "scripts": {
    "test": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- --recursive",
    "coveralls": "node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --recursive && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  }
}
